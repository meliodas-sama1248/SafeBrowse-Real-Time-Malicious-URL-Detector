{
    "manifest_version": 3,
    "name": "SafeBrowse: Real-Time Malicious URL Detector",
    "version": "1.0",
    "description": "Detects and blocks malicious URLs in real-time.",
    "permissions": ["webNavigation", "tabs", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "32": "icons/icon32.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
      }
    }
  }
  